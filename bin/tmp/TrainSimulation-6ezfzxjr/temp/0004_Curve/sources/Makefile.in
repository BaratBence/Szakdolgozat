PHONY: TrainSimulation_Curve_FMU
TrainSimulation_Curve_FMU: nozip
	cd .. && rm -f ../TrainSimulation_Curve.fmu && zip -r ../TrainSimulation.Curve.fmu *
nozip: TrainSimulation_Curve_functions.h TrainSimulation_Curve_literals.h $(OFILES) $(RUNTIMEFILES)
	$(CXX) -shared -I. -o TrainSimulation_Curve$(DLLEXT) $(RUNTIMEFILES) $(OFILES) $(CPPFLAGS) -L"E:/Szakdoga"   $(CFLAGS) $(LDFLAGS) -llis -Wl,--kill-at
	mkdir.exe -p ../binaries/win64
	dlltool -d TrainSimulation_Curve.def --dllname TrainSimulation_Curve$(DLLEXT) --output-lib TrainSimulation_Curve.lib --kill-at
	cp TrainSimulation_Curve$(DLLEXT) TrainSimulation_Curve.lib TrainSimulation_Curve_FMU.libs ../binaries/win64/
	rm -f *.o TrainSimulation_Curve$(DLLEXT) $(OFILES) $(RUNTIMEFILES)
	cd .. && rm -f ../TrainSimulation_Curve.fmu && zip -r ../TrainSimulation.Curve.fmu *

